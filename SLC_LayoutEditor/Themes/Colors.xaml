    <SolidColorBrush x:Key="AisleColorBrush" Color="{StaticResource BackgroundDark}" Opacity=".6"/>
    <SolidColorBrush x:Key="CockpitColorBrush" Color="{StaticResource CockpitColor}"/>
    <SolidColorBrush x:Key="CrewAreaColorBrush" Color="{StaticResource CrewAreaColor}" Opacity=".5"/>
    <SolidColorBrush x:Key="CrewAreaBorderColorBrush" Color="{StaticResource CrewAreaColor}"/>
    <SolidColorBrush x:Key="FirstClassColorBrush" Color="{StaticResource FirstClassColor}"/>
    <SolidColorBrush x:Key="BusinessClassColorBrush" Color="{StaticResource BusinessClassColor}"/>
    <SolidColorBrush x:Key="PremiumClassColorBrush" Color="{StaticResource PremiumClassColor}"/>
    <SolidColorBrush x:Key="EconomyClassColorBrush" Color="{StaticResource EconomyClassColor}"/>
    <SolidColorBrush x:Key="SupersonicClassColorBrush" Color="{StaticResource SupersonicClassColor}"/>
    <SolidColorBrush x:Key="UnavailableSeatColorBrush" Color="{StaticResource UnavailableSeatColor}"/>
    <SolidColorBrush x:Key="ToiletColorBrush" Color="{StaticResource ToiletColor}"/>
    <SolidColorBrush x:Key="StairwellColorBrush" Color="{StaticResource StairwellColor}" Opacity=".5"/>
    <SolidColorBrush x:Key="StairwellBorderColorBrush" Color="{StaticResource StairwellColor}"/>
    <SolidColorBrush x:Key="WallColorBrush" Color="{StaticResource WallColor}" Opacity=".5"/>
    <SolidColorBrush x:Key="WallColorBorderBrush" Color="{StaticResource WallColor}"/>
    <SolidColorBrush x:Key="DoorColorBrush" Color="{StaticResource DoorColor}"/>
    <SolidColorBrush x:Key="CateringDoorColorBrush" Color="{StaticResource CateringDoorColor}"/>
    <SolidColorBrush x:Key="LoadingBayColorBrush" Color="{StaticResource LoadingBayColor}"/>
    <SolidColorBrush x:Key="KitchenColorBrush" Color="{StaticResource KitchenColor}"/>
    <SolidColorBrush x:Key="IntercomColorBrush" Color="{StaticResource IntercomColor}"/>
    <SolidColorBrush x:Key="ServicePointStartColorBrush" Color="{StaticResource ServicePointStartColor}"/>
    <SolidColorBrush x:Key="ServicePointEndColorBrush" Color="{StaticResource ServicePointEndColor}"/>

    <SolidColorBrush x:Key="HeartColorBrush" Color="{StaticResource HeartColor}"/>
    <SolidColorBrush x:Key="HeartHoverColorBrush" Color="{StaticResource HeartLightColor}" Opacity=".24"/>
    <SolidColorBrush x:Key="HeartPressedColorBrush" Color="{StaticResource HeartDarkColor}" Opacity=".40"/>

    <LinearGradientBrush x:Key="SerivePointsGradientBrush" EndPoint="20,0" StartPoint="0,20" 
                         MappingMode="Absolute" SpreadMethod="Repeat">
        <GradientStop Color="{StaticResource ServicePointStartColor}" Offset="0"/>
        <GradientStop Color="{StaticResource ServicePointStartColor}" Offset=".45"/>
        <GradientStop Color="{StaticResource ServicePointEndColor}" Offset=".55"/>
        <GradientStop Color="{StaticResource ServicePointEndColor}" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="WindowBorderBrush" Color="{StaticResource BackgroundLight}"/>
    
    <SolidColorBrush x:Key="BackgroundColorBrush" Color="{StaticResource BackgroundColor}"/>
    <SolidColorBrush x:Key="BackgroundLightBrush" Color="{StaticResource BackgroundLight}"/>
    <SolidColorBrush x:Key="BackgroundLightHoverBrush" Color="{StaticResource BackgroundLight}" Opacity=".24"/>
    <SolidColorBrush x:Key="BackgroundLightPressedBrush" Color="{StaticResource BackgroundDark}" Opacity=".24"/>
    <SolidColorBrush x:Key="BackgroundDarkBrush" Color="{StaticResource BackgroundDark}"/>
    <SolidColorBrush x:Key="MenuBackgroundBrush" Color="{StaticResource BackgroundLight}" Opacity=".2"/>
    <SolidColorBrush x:Key="ForegroundColorBrush" Color="{StaticResource ForegroundColor}" Opacity=".87"/>
    <SolidColorBrush x:Key="TitleForefroundColorBrush" Color="{StaticResource ForegroundColor}" Opacity=".87"/>
    <SolidColorBrush x:Key="ForegroundHoverColorBrush" Color="{StaticResource ForegroundColor}" Opacity=".95"/>
    <SolidColorBrush x:Key="ForegroundPressedColorBrush" Color="{StaticResource ForegroundSecondaryColor}" Opacity=".6"/>
    <SolidColorBrush x:Key="ForegroundDarkColorBrush" Color="{StaticResource ForegroundDarkColor}" Opacity=".87"/>
    <SolidColorBrush x:Key="ForegroundDarkHoverColorBrush" Color="{StaticResource ForegroundSecondaryDarkColor}" Opacity=".95"/>
    <SolidColorBrush x:Key="ForegroundDarkPressedColorBrush" Color="{StaticResource ForegroundDarkColor}" Opacity=".95"/>
    <SolidColorBrush x:Key="HyperlinkColorBrush" Color="{StaticResource HyperlinkColor}" Opacity=".87"/>
    <SolidColorBrush x:Key="HyperlinkHoverColorBrush" Color="{StaticResource HyperlinkHoverColor}" Opacity=".87"/>
    <SolidColorBrush x:Key="HyperlinkFocusedColorBrush" Color="{StaticResource AccentPressedColor}" Opacity=".87"/>

    <SolidColorBrush x:Key="SeparatorColorBrush" Color="{StaticResource SeparatorColor}"/>
    <SolidColorBrush x:Key="LiveGuideOverlayBrush" Color="{StaticResource GuideBackdropColor}" Opacity=".7"/>
    <SolidColorBrush x:Key="TabControlSeparatorColorBrush" Color="{StaticResource BackgroundLight}"/>
    <SolidColorBrush x:Key="TabItemBorderSelectedColorBrush" Color="{StaticResource AccentPressedColor}" Opacity=".6"/>
    <SolidColorBrush x:Key="TabItemBorderHoverColorBrush" Color="{StaticResource AccentHoverColor}" Opacity=".6"/>

    <SolidColorBrush x:Key="RemoveDeckButtonBackgroundBrush" Color="{StaticResource BackgroundColor}" Opacity=".24"/>
    <SolidColorBrush x:Key="RemoveDeckButtonHoverBackgroundBrush" Color="{StaticResource BackgroundLight}" Opacity=".24"/>
    <SolidColorBrush x:Key="RemoveDeckButtonPressedBackgroundBrush" Color="{StaticResource BackgroundDark}" Opacity=".24"/>
    <SolidColorBrush x:Key="AddDeckButtonBackgroundBrush" Color="{StaticResource BackgroundColor}" Opacity=".48"/>
    <SolidColorBrush x:Key="AddDeckButtonHoverBackgroundBrush" Color="{StaticResource BackgroundLight}" Opacity=".48"/>
    <SolidColorBrush x:Key="AddDeckButtonPressedBackgroundBrush" Color="{StaticResource BackgroundDark}" Opacity=".48"/>

    <SolidColorBrush x:Key="ForegroundSecondaryColorBrush" Color="{StaticResource ForegroundSecondaryColor}"/>
    <SolidColorBrush x:Key="ToolbarButtonHoverColorBrush" Color="{StaticResource ForegroundSecondaryColor}" Opacity=".27"/>
    <SolidColorBrush x:Key="ToolbarButtonPressedColorBrush" Color="{StaticResource ForegroundColor}" Opacity=".35"/>
    
    <SolidColorBrush x:Key="ErrorForegroundColorBrush" Color="{StaticResource ErrorColor}"/>
    <SolidColorBrush x:Key="WarnForegroundColorBrush" Color="{StaticResource WarnColor}"/>
    <SolidColorBrush x:Key="ValidForegroundColorBrush" Color="{StaticResource ButtonBorderColor}"/>

    <LinearGradientBrush x:Key="ErrorHighlightColorBrush" EndPoint="11,0" StartPoint="0,11" 
                         MappingMode="Absolute" SpreadMethod="Repeat" Opacity=".55">
        <GradientStop Color="Transparent" Offset="0"/>
        <GradientStop Color="Transparent" Offset=".5"/>
        <GradientStop Color="{StaticResource ErrorDarkColor}" Offset=".5"/>
        <GradientStop Color="{StaticResource ErrorDarkColor}" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="ErrorSelectedHighlightColorBrush" EndPoint="11,0" StartPoint="0,11" 
                         MappingMode="Absolute" SpreadMethod="Repeat" Opacity=".55">
        <GradientStop Color="#45FFFFFF" Offset="0"/>
        <GradientStop Color="#45FFFFFF" Offset=".5"/>
        <GradientStop Color="{StaticResource ErrorColor}" Offset=".5"/>
        <GradientStop Color="{StaticResource ErrorColor}" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="DisabledColorBrush" Color="{StaticResource ForegroundColor}" Opacity=".27"/>
    <SolidColorBrush x:Key="DisabledDarkColorBrush" Color="{StaticResource ForegroundDarkColor}" Opacity=".27"/>
    <SolidColorBrush x:Key="BackdropColorBrush" Color="{StaticResource BackgroundColor}" Opacity=".6"/>
    <SolidColorBrush x:Key="BackdropDarkColorBrush" Color="{StaticResource BackgroundColor}" Opacity=".9"/>
    <SolidColorBrush x:Key="MenuItemHoverColorBrush">#333333</SolidColorBrush>

