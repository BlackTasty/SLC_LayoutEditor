<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <BlurEffect x:Key="DisableBlur" Radius="10" KernelType="Gaussian"></BlurEffect>

    <DropShadowEffect x:Key="DropShadowDepth1" Color="{DynamicResource BackgroundDark}" Opacity=".4"
                      ShadowDepth="1" BlurRadius="4" Direction="270" RenderingBias="Performance"/>
    <DropShadowEffect x:Key="DropShadowDepth2" Color="{DynamicResource BackgroundDark}" Opacity=".4"
                      ShadowDepth="2" BlurRadius="10" Direction="270" RenderingBias="Performance"/>

    <Color x:Key="CockpitColor">#dd00ffe0</Color>
    <Color x:Key="CrewAreaColor">#800094ff</Color>
    <Color x:Key="FirstClassColor">#ccffd700</Color>
    <Color x:Key="BusinessClassColor">#cc0094ff</Color>
    <Color x:Key="PremiumClassColor">#ccffffff</Color>
    <Color x:Key="EconomyClassColor">#cc808080</Color>
    <Color x:Key="SupersonicClassColor">#ccdaa520</Color>
    <Color x:Key="UnavailableSeatColor">#cc333333</Color>
    <Color x:Key="ToiletColor">#cc663399</Color>
    <Color x:Key="StairwellColor">#60ffab00</Color>
    <Color x:Key="WallColor">#40cc1000</Color>
    <Color x:Key="DoorColor">#00ff90</Color>
    <Color x:Key="CateringDoorColor">#666</Color>
