    <Color x:Key="ForegroundColor">#FFF</Color>
    <Color x:Key="ForegroundSecondaryColor">#C9C9C9</Color>
    <Color x:Key="ForegroundDarkColor">#222</Color>
    <Color x:Key="ForegroundSecondaryDarkColor">#555</Color>
    <Color x:Key="HyperlinkColor">#40afff</Color>
    <Color x:Key="HyperlinkHoverColor">#80caff</Color>
    <Color x:Key="HyperlinkPressedColor">#2485cb</Color>

    <Color x:Key="GuideBackdropColor">#030304</Color>

