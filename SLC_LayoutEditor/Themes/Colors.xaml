    <SolidColorBrush x:Key="LayoutButtonBackgroundHoverColorBrush" Color="{StaticResource ButtonBackgroundHoverColor}"/>
    <SolidColorBrush x:Key="LayoutButtonBackgroundPressedColorBrush" Color="{StaticResource ButtonBackgroundPressedColor}"/>

