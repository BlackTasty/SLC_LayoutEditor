    <SolidColorBrush x:Key="CheckBoxMarkHoverColorBrush" Color="{StaticResource AccentHoverColor}"/>
    <SolidColorBrush x:Key="CheckBoxMarkPressedColorBrush" Color="{StaticResource AccentPressedColor}"/>
    <SolidColorBrush x:Key="ComboBoxItemSplitterColorBrush" Color="{StaticResource AccentColor}"/>
    <SolidColorBrush x:Key="ComboBoxBorderColorBrush" Color="{StaticResource ButtonBorderColor}"/>
    <SolidColorBrush x:Key="ComboBoxBorderHoverColorBrush" Color="{StaticResource ButtonBorderHoverColor}"/>
    <SolidColorBrush x:Key="ComboBoxBorderPressedColorBrush" Color="{StaticResource ButtonBorderPressedColor}"/>

    <SolidColorBrush x:Key="AccentColorBrush" Color="{StaticResource AccentColor}"/>
    <SolidColorBrush x:Key="AccentHoverColorBrush" Color="{StaticResource AccentHoverColor}"/>
    <SolidColorBrush x:Key="AccentPressedColorBrush" Color="{StaticResource AccentPressedColor}"/>

    <SolidColorBrush x:Key="NotificationProgressForeground" Color="{StaticResource AccentPressedColor}" Opacity=".17"/>

    <SolidColorBrush x:Key="AccentBackColorBrush" Color="{StaticResource AccentColor}" Opacity=".6"/>
    <SolidColorBrush x:Key="AccentBackHoverColorBrush" Color="{StaticResource AccentHoverColor}" Opacity=".6"/>
    <SolidColorBrush x:Key="AccentBackPressedColorBrush" Color="{StaticResource AccentPressedColor}" Opacity=".6"/>

    <SolidColorBrush x:Key="ButtonWarnBorderColorBrush" Color="{StaticResource WarnColor}"/>
    <SolidColorBrush x:Key="ButtonWarnBorderHoverColorBrush" Color="{StaticResource WarnLightColor}"/>
    <SolidColorBrush x:Key="ButtonWarnBorderPressedColorBrush" Color="{StaticResource ButtonWarnBorderPressedColor}"/>
    <SolidColorBrush x:Key="ButtonWarnBackgroundHoverColorBrush" Color="{StaticResource ButtonWarnBackgroundHoverColor}"/>
    <SolidColorBrush x:Key="ButtonWarnBackgroundPressedColorBrush" Color="{StaticResource ButtonWarnBackgroundPressedColor}"/>

    <SolidColorBrush x:Key="ButtonErrorBorderColorBrush" Color="{StaticResource ErrorColor}"/>
    <SolidColorBrush x:Key="ButtonErrorBorderPressedColorBrush" Color="{StaticResource ErrorDarkColor}"/>
    <SolidColorBrush x:Key="ButtonErrorBackgroundHoverColorBrush" Color="{StaticResource ErrorLightColor}" Opacity=".24"/>
    <SolidColorBrush x:Key="ButtonErrorBackgroundPressedColorBrush" Color="{StaticResource ButtonErrorBackgroundPressedColor}"/>

    <SolidColorBrush x:Key="RemoveDeckHoverBrush" Color="{StaticResource ErrorLightColor}"/>
    <SolidColorBrush x:Key="NoLayoutsColorBrush" Color="{StaticResource WarnLightColor}" Opacity=".5"/>

