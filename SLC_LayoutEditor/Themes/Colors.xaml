    <SolidColorBrush x:Key="TemplatingModeBorderBrush" Color="{StaticResource TemplatingModeColor}"/>
    <SolidColorBrush x:Key="TemplatingModeBorderHoverBrush" Color="{StaticResource TemplatingModeHoverColor}"/>
    <SolidColorBrush x:Key="TemplatingModeBorderPressedBrush" Color="{StaticResource TemplatingModePressedColor}"/>
    <SolidColorBrush x:Key="TemplatingModeBrush" Color="{StaticResource TemplatingModeColor}"/>
    <SolidColorBrush x:Key="TemplatingModeHoverBrush" Color="{StaticResource TemplatingModeHoverColor}"/>
    <SolidColorBrush x:Key="TemplatingModePressedBrush" Color="{StaticResource TemplatingModePressedColor}"/>
    <SolidColorBrush x:Key="TemplatingModeBackgroundBrush" Color="{StaticResource TemplatingModeColor}" Opacity=".24"/>
    <SolidColorBrush x:Key="TemplatingModeBackgroundHoverBrush" Color="{StaticResource TemplatingModeHoverColor}" Opacity=".24"/>
    <SolidColorBrush x:Key="TemplatingModeBackgroundPressedBrush" Color="{StaticResource TemplatingModePressedColor}" Opacity=".24"/>

    <LinearGradientBrush x:Key="TemplatingModeBackBrush" EndPoint="16,0" StartPoint="0,16" Opacity=".025"
                         MappingMode="Absolute" SpreadMethod="Repeat">
        <GradientStop Color="#45FFFFFF" Offset="0"/>
        <GradientStop Color="#45FFFFFF" Offset=".5"/>
        <GradientStop Color="{StaticResource TemplatingModeColor}" Offset=".5"/>
        <GradientStop Color="{StaticResource TemplatingModeColor}" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="TemplatingModeTitleBrush" EndPoint="16,0" StartPoint="0,16" Opacity="1"
                         MappingMode="Absolute" SpreadMethod="Repeat">
        <GradientStop Color="{StaticResource BackgroundLight}" Offset="0"/>
        <GradientStop Color="{StaticResource BackgroundLight}" Offset=".5"/>
        <GradientStop Color="#714723" Offset=".5"/>
        <GradientStop Color="#714723" Offset="1"/>
